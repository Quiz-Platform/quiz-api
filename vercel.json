{
  "version": 3,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/questions",
      "dest": "/api/questions"
    },
    {
      "src": "/api/questions/([^/]+)",
      "dest": "/api/questions/[id]"
    },
    {
      "src": "/api/answers",
      "dest": "/api/answers"
    },
    {
      "src": "/api/answers/stats",
      "dest": "/api/answers/stats"
    },
    {
      "src": "/api/web-hooks/telegram-bot",
      "dest": "/api/web-hooks/telegram-bot"
    }
  ]
}
